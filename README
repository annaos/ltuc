#!/usr/bin/env less

# Repository herunterladen

    git clone https://github.com/mdrone/ltuc.git 

# Aenderungen machen
# Kommentieren

    git commit -am 'Kommentar'

# Aenderungen Hochladen

    git push origin master

# Neue Dateien Hinzufuegen

    git add ./Datei

    git commit -am 'Kommentar'

    git push origin master

# Update herunterladen

    git pull



# Script zum testen mit unterschiedlichen Tabellengroessen

    generator 300

# fuehrt Programm 300 mal aus mit dem Argument 1 bis 300
